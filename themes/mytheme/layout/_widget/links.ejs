<% if (theme.links){ %>
  <div class="widget-wrap">
    <h3 class="widget-title"><%= __('友链') %></h3>
    <div class="widget">
      <ul class="entry">
        <% for (var i in theme.links){ %>
          <li class='link'><a target="_blank" href='<%- theme.links[i] %>'><%= i %></a></li>
        <% } %>
      </ul>
    </div>
  </div>
<% } %>